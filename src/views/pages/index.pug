extends ../layouts/layout.pug

block head
    title Miliday template

block style
    +style('./assets/css/main.css')

block main
    section.b-page
        header.b-header.b-page__header
            .b-header-desk.b-header__desk
                p header-desk

            .b-header-mob.b-header__mob
                p header-mob
        
        main.b-main.b-page__main
            p main
        
        footer.b-footer
            p footer

block script
    +script('./assets/js/main.js')